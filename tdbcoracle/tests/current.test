    }
    -returnCodes error
    -match glob
    -result {wrong # args*}
}

test tdbc::oracle-1.3 {create a connection, bad arg} {*}{
    -body {
	tdbc::oracle::connection create db -rubbish rubbish
    }
    -returnCodes error
    -match glob
    -result {bad option "-rubbish"*}
}

